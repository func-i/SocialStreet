$('#list-view-explore-content').html('<%= escape_javascript(render(:partial => 'results')) %>');

markerManager.deleteAllMarkers();

<% (@events || []).each do |event| %>
  createExploreMarker(parseFloat(<%=event.location.latitude%>, 10), parseFloat(<%=event.location.longitude%>, 10), <%=event.id%>);
<% end %>

markerManager.showAllMarkers();