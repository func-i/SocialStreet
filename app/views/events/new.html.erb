<div class="block">
  <div class="secondary-navigation">
    <ul class="wat-cf">
      <li class="first active"><a href="#basic" data-tab="#basic">Basic Info</a></li>
      <li><a href="#details" data-tab="#details">Details</a></li>
      <li><a href="#attendees" data-tab="#attendees">Attendee Options</a></li>
      <li><a href="#cost" data-tab="#cost">Cost</a></li>
    </ul>
  </div>
  <div class="content">
    <h2 class="title">Create Event</h2>
    <div class="inner">
      <%= form_for @event, :html => {:class => "form"} do |form| %>
        <%= render form %>
        <div class="group navform wat-cf">
          <button class="button" type="submit">
            <img src="/images/web-app-theme/icons/tick.png" alt="Save" /> Save
          </button>
          <a href="#header" class="button">
            <img src="/images/web-app-theme/icons/cross.png" alt="Cancel"/> Cancel
          </a>
        </div>

      <% end %>
    </div>
  </div>
</div>