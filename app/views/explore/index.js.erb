$('#num-results').text('<%= @searchables.size %>');

<%- if @searchables.size < @per_page -%>
  $('#comment_suggest').show();
<%- else -%>
  $('#comment_suggest').hide();
<%- end -%>

$('#action-list').html('<%= escape_javascript(render('results')) %>');
clearMarkers();
<% @searchables.each do |searchable| %>
  <% if searchable.geo_located? %>
    placeMarker(new google.maps.LatLng(<%= searchable.location.latitude %>, <%= searchable.location.longitude %>), '#');
  <% end %>
<% end %>
makePageless(<%= @num_pages %>, <%= params[:page] || 1 %>);