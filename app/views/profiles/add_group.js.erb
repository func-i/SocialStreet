<% if @success %>
  hideGroupPermissionHolder();

  if(isOnShowEvent()){
    rsvpToEvent();
  }
  else if(isOnSettings()){
    addKeywordToHolder($('#group_permission_name').val() ,$('#group_permission_icon_class').val());
  }
  else if(isOnShowGroup(){
    $('#join_group_btn').addClass('hidden');
  }
<% else %>
  $('#group_permission_error').removeClass('hidden');
<% end %>